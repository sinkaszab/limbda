{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "rules": {
    "no-console": 0,
    "no-nested-ternary": 0,
    "no-plusplus": 0,
    "eqeqeq": 0,
    "no-underscore-dangle": 0,
    "prefer-rest-params": 0
  },
  "plugins": [
    "flowtype"
  ]
}
